{
  "name": "kigs_pos",
  "version": "1.0.0",
  "description": "This template should help get you started developing with Vue 3 in Vite.",
  "main": "jest.config.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "vite build",
    "test:unit": "jest --passWithNoTests",
    "dev": "vite",
    "start": "vite",
    "preview": "vite preview --port 4173",
    "test": "jest",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint --ext .js,.vue src"
  },
  "dependencies": {
    "register-service-worker": "^1.7.2",
    "tw-elements": "^1.0.0-beta1",
    "vue": "^2.7.16",
    "vue-router": "^3.6.5",
    "vuex": "^3.6.2"
  },
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.[jt]s?(x)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!your-module-to-transform)"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-typescript": "^7.25.7",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.19.56",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@vitejs/plugin-legacy": "^2.3.1",
    "@vitejs/plugin-vue2": "^1.1.2",
    "@vue/babel-plugin-jsx": "^1.2.5",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/test-utils": "^1.3.6",
    "@vue/tsconfig": "^0.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.2",
    "autoprefixer": "^10.4.14",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.5.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-vue": "^9.29.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.0",
    "terser": "^5.14.2",
    "ts-jest": "^27.x",
    "typescript": "^5.6.3",
    "vite": "^3.2.11",
    "vue-jest": "^3.0.7",
    "vue-tsc": "^0.38.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bse23-4/KIGS_POS.git"
  },
